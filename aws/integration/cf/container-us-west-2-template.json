				"UserData": {
				  "Fn::Base64": { "Fn::Join": [ "", [
					"#!/bin/bash -ex\n",
					"exec > >(tee /var/log/user-data.log|logger -t user-data -s 2>/dev/console) 2>&1\n",
					"yum install -y aws-cfn-bootstrap\n",				  
					"# Remainder of configuration via metadata\n",
					"/opt/aws/bin/cfn-init -v",
					"         --stack ", { "Ref" : "AWS::StackName" },
